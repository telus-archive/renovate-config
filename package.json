{
  "version": "1.1.3",
  "name": "@telusdigital/renovate-config",
  "description": "TELUS preset configs for Renovate",
  "author": "Ahmad Nassri <ahmad.nassri@telus.com> (https://labs.telus.com/)",
  "homepage": "https://github.com/telusdigital/renovate-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/telusdigital/renovate-config.git"
  },
  "license": "ISC",
  "keywords": [
    "telus",
    "renovate",
    "config"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/telusdigital/renovate-config/issues"
  },
  "scripts": {
    "test": "echo \"WARNING: testing not applicable\" && exit 0"
  },
  "renovate-config": {
    "default": {
      "extends": [
        "config:js-app",
        "schedule:weekly"
      ],
      "pinVersions": false,
      "separatePatchReleases": true,
      "lockFileMaintenance": {
        "enabled": true,
        "extends": "schedule:weekly"
      },
      "labels": [
        "dependencies"
      ],
      "prHourlyLimit": 0,
      "prConcurrentLimit": 0,
      "semanticCommits": true,
      "rebaseStalePrs": true,
      "ignoreNpmrcFile": true,
      "encrypted": {
        "npmToken": ""
      }
    }
  }
}
